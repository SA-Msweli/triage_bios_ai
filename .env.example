# Triage-BIOS.ai Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# Google Gemini AI Configuration
# =============================================================================
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_BASE_URL=https://generativelanguage.googleapis.com
GEMINI_API_VERSION=v1beta
GEMINI_MODEL_ID=gemini-1.5-flash

# =============================================================================
# IBM Cloud Configuration
# =============================================================================
IBM_CLOUD_API_KEY=your_ibm_cloud_api_key_here
IBM_CLOUD_REGION=us-south
IBM_BLOCKCHAIN_SERVICE_URL=your_blockchain_service_url_here

# =============================================================================
# Firebase Configuration (NEW)
# =============================================================================
# Web Configuration
FIREBASE_WEB_API_KEY=your-firebase-web-api-key
FIREBASE_WEB_APP_ID=1:123456789:web:abcdef123456
FIREBASE_PROJECT_ID=triagebiosai
FIREBASE_AUTH_DOMAIN=triagebiosai.firebaseapp.com
FIREBASE_STORAGE_BUCKET=triagebiosai.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Android Configuration
FIREBASE_ANDROID_API_KEY=your-firebase-android-api-key
FIREBASE_ANDROID_APP_ID=1:123456789:android:abcdef123456

# iOS Configuration
FIREBASE_IOS_API_KEY=your-firebase-ios-api-key
FIREBASE_IOS_APP_ID=1:123456789:ios:abcdef123456
FIREBASE_IOS_BUNDLE_ID=com.example.triageBiosAi

# =============================================================================
# Hospital Data Configuration
# =============================================================================
# Hospital data will now be managed through Firebase/Firestore

# =============================================================================
# Wearable Device Integration
# =============================================================================
# Apple HealthKit (iOS only - configured in Info.plist)
HEALTHKIT_ENABLED=true

# Google Health Connect
GOOGLE_HEALTH_CONNECT_CLIENT_ID=your_google_health_connect_client_id_here
GOOGLE_HEALTH_CONNECT_ENABLED=true

# Samsung Health
SAMSUNG_HEALTH_APP_ID=your_samsung_health_app_id_here
SAMSUNG_HEALTH_ENABLED=true

# Fitbit Web API
FITBIT_CLIENT_ID=your_fitbit_client_id_here
FITBIT_CLIENT_SECRET=your_fitbit_client_secret_here
FITBIT_ENABLED=true

# Garmin Connect IQ
GARMIN_CONSUMER_KEY=your_garmin_consumer_key_here
GARMIN_CONSUMER_SECRET=your_garmin_consumer_secret_here
GARMIN_ENABLED=false

# Oura Ring
OURA_CLIENT_ID=your_oura_client_id_here
OURA_CLIENT_SECRET=your_oura_client_secret_here
OURA_ENABLED=false

# =============================================================================
# Maps and Location Services
# =============================================================================
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
HERE_MAPS_API_KEY=your_here_maps_api_key_here

# African-specific mapping services
AFRICA_MAPPING_API_KEY=your_africa_mapping_api_key_here
OPENSTREETMAP_API_KEY=your_openstreetmap_api_key_here

# =============================================================================
# Emergency Services Integration
# =============================================================================
EMERGENCY_DISPATCH_API_URL=https://api.emergency-dispatch.gov/v1
EMERGENCY_DISPATCH_API_KEY=your_emergency_dispatch_api_key_here
AMBULANCE_ROUTING_API_URL=https://api.ambulance-routing.com/v1
AMBULANCE_ROUTING_API_KEY=your_ambulance_routing_api_key_here

# African Emergency Services
AFRICA_EMERGENCY_SERVICES_API_URL=https://api.africa-emergency.org/v1
AFRICA_EMERGENCY_SERVICES_API_KEY=your_africa_emergency_api_key_here

# Country-specific emergency numbers and services
NIGERIA_EMERGENCY_API_URL=https://api.nema.gov.ng/v1
SOUTH_AFRICA_EMERGENCY_API_URL=https://api.saps.gov.za/emergency/v1
KENYA_EMERGENCY_API_URL=https://api.kenya-emergency.go.ke/v1
GHANA_EMERGENCY_API_URL=https://api.nadmo.gov.gh/v1
ETHIOPIA_EMERGENCY_API_URL=https://api.ethiopia-emergency.gov.et/v1

# =============================================================================
# Database Configuration
# =============================================================================
# PostgreSQL (Production)
DATABASE_URL=postgresql://username:password@localhost:5432/triage_bios_ai
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=triage_bios_ai
DATABASE_USER=your_db_username
DATABASE_PASSWORD=your_db_password

# SQLite (Development/Demo)
SQLITE_DATABASE_PATH=./data/triage_bios_ai.db

# Redis Cache
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# =============================================================================
# Authentication & Security
# =============================================================================
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_random
JWT_EXPIRATION_HOURS=24
ENCRYPTION_KEY=your_encryption_key_32_chars_long
BCRYPT_ROUNDS=12

# OAuth Providers
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret_here
APPLE_OAUTH_CLIENT_ID=your_apple_oauth_client_id_here
APPLE_OAUTH_CLIENT_SECRET=your_apple_oauth_client_secret_here

# =============================================================================
# Blockchain & Consent Management
# =============================================================================
HYPERLEDGER_FABRIC_NETWORK_URL=your_hyperledger_network_url_here
HYPERLEDGER_FABRIC_USER_ID=your_fabric_user_id_here
HYPERLEDGER_FABRIC_USER_SECRET=your_fabric_user_secret_here
IPFS_GATEWAY_URL=https://ipfs.io/ipfs/
IPFS_API_URL=https://api.pinata.cloud/pinning/pinFileToIPFS
IPFS_API_KEY=your_ipfs_api_key_here

# =============================================================================
# Payment & Monetization (RevenueCat)
# =============================================================================
REVENUECAT_PUBLIC_API_KEY=your_revenuecat_public_api_key_here
REVENUECAT_SECRET_API_KEY=your_revenuecat_secret_api_key_here
REVENUECAT_APP_USER_ID=your_revenuecat_app_user_id_here

# Government ID Verification
GOVERNMENT_ID_VERIFICATION_API_URL=https://api.id-verification.gov/v1
GOVERNMENT_ID_VERIFICATION_API_KEY=your_gov_id_api_key_here

# Insurance Verification
INSURANCE_VERIFICATION_API_URL=https://api.insurance-verification.com/v1
INSURANCE_VERIFICATION_API_KEY=your_insurance_api_key_here

# African Government ID Verification Services
NIGERIA_NIN_VERIFICATION_URL=https://api.nimc.gov.ng/v1/verify
SOUTH_AFRICA_ID_VERIFICATION_URL=https://api.dha.gov.za/v1/verify
KENYA_ID_VERIFICATION_URL=https://api.hudumakenya.go.ke/v1/verify
GHANA_ID_VERIFICATION_URL=https://api.nia.gov.gh/v1/verify
ETHIOPIA_ID_VERIFICATION_URL=https://api.nidp.gov.et/v1/verify
EGYPT_ID_VERIFICATION_URL=https://api.egypt.gov.eg/v1/verify
MOROCCO_ID_VERIFICATION_URL=https://api.cnie.ma/v1/verify
RWANDA_ID_VERIFICATION_URL=https://api.nida.gov.rw/v1/verify

# African Healthcare Insurance Systems
NIGERIA_NHIS_API_URL=https://api.nhis.gov.ng/v1
SOUTH_AFRICA_MEDICAL_SCHEMES_API_URL=https://api.medicalschemes.co.za/v1
KENYA_NHIF_API_URL=https://api.nhif.or.ke/v1
GHANA_NHIS_API_URL=https://api.nhis.gov.gh/v1
RWANDA_CBHI_API_URL=https://api.cbhi.gov.rw/v1

# =============================================================================
# Notification Services
# =============================================================================
# Firebase Cloud Messaging
FCM_SERVER_KEY=your_fcm_server_key_here
FCM_SENDER_ID=your_fcm_sender_id_here

# Apple Push Notifications
APNS_KEY_ID=your_apns_key_id_here
APNS_TEAM_ID=your_apns_team_id_here
APNS_BUNDLE_ID=com.triageBiosAi.app

# SMS Notifications (Twilio)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Email Notifications (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM_EMAIL=noreply@triage-bios.ai

# =============================================================================
# Analytics & Monitoring
# =============================================================================
# Application Performance Monitoring
SENTRY_DSN=your_sentry_dsn_here
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key_here

# Analytics
GOOGLE_ANALYTICS_TRACKING_ID=your_ga_tracking_id_here
MIXPANEL_PROJECT_TOKEN=your_mixpanel_token_here

# =============================================================================
# Development & Testing
# =============================================================================
# Environment
NODE_ENV=development
FLUTTER_ENV=development
DEBUG_MODE=true
LOG_LEVEL=debug

# Mock Services (for development)
USE_MOCK_GEMINI=true
USE_MOCK_WEARABLES=true
USE_MOCK_PAYMENTS=true
USE_FIREBASE=true

# Test Configuration
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/triage_bios_ai_test
TEST_REDIS_URL=redis://localhost:6380

# =============================================================================
# Feature Flags
# =============================================================================
ENABLE_BLOCKCHAIN_CONSENT=false
ENABLE_EMERGENCY_DISPATCH=false
ENABLE_PAYMENT_PROCESSING=false
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_MULTI_LANGUAGE=true
ENABLE_OFFLINE_MODE=true
ENABLE_VOICE_INPUT=true
ENABLE_IMAGE_ANALYSIS=true

# =============================================================================
# Rate Limiting & Performance
# =============================================================================
API_RATE_LIMIT_PER_MINUTE=100
GEMINI_TIMEOUT_SECONDS=30
CACHE_TTL_SECONDS=300
MAX_CONCURRENT_REQUESTS=50

# =============================================================================
# Compliance & Audit
# =============================================================================
HIPAA_COMPLIANCE_MODE=true
GDPR_COMPLIANCE_MODE=true
AUDIT_LOG_RETENTION_DAYS=2555  # 7 years
DATA_ENCRYPTION_AT_REST=true
DATA_ENCRYPTION_IN_TRANSIT=true